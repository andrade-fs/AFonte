{
  "compilerOptions": {
    "module": "es6",
    "target": "es5",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
  },
  "include": ["src"],
  "references": [{ "path": "./tsconfig.node.json" }]
}
